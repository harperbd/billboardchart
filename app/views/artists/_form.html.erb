<%= @artist.id ? "Edit Artist" : "Add New Artist" %>

<%= form_for [@artist] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.submit %>
  <% end %>
<br/>

<% @artist_list = ["Adele", "Santan"] %>

<%= "Or choose existing artist:"%>
<%= form_for [@artist] do |f| %>
  <%= f.select :name, [
    @artist_list.each do |artist|
    "#{artist}"
    end
  ]%>
<% end %>