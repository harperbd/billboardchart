<%#= todo: load billboard to include the billboard name %>
<%= form_for @billboard_song_ranking do |f| %>
  <%#= todo: set hidden billboard_id??? %>

  <%= f.label :song_id %>
  <%#= todo: also include artist name in select box? %>
  <%= f.select(:song_id, options_from_collection_for_select(@songs, :id, :title)) %><br />

  <%#= todo: select box of 1-100 or whatever the billboard is (e.g., Top 100, Top 50, etc) %>
  <%= f.label :ranking %>
  <%= f.select :ranking, (1..100).map {|i| [i,i] } %><br />
  <%= f.submit %>
<% end %>