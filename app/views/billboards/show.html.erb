<h1><%= @billboard.name %></h1>

    <% @billboard.songs.each do |song| %>
    <%= "Title: #{song.title}" %>
    <%= "Artist: #{song.artist.name}" %>
    <%= "Current Rank: #{song.rank}" %>
    <br/>
    <% end %>

  <%= link_to "Add New Song To List", new_billboard_artist_path(@billboard) %> 
  <%= link_to "Delete List", @billboard, method: :delete %>
